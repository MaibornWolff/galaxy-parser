{
    "name": "node-parser",
    "version": "1.0.0",
    "description": "Node based parser to calculate metrics for different languages",
    "main": "dist/app.js",
    "scripts": {
        "start": "tsc && node dist/app.js parse",
        "format": "prettier --write \"./**/*\"",
        "format:quick": "pretty-quick --staged",
        "lint": "eslint \"src/**/*.ts\"",
        "lint:fix": "npm run lint -- --fix",
        "lint:quick": "npm run precommit",
        "prepare": "husky install",
        "precommit": "lint-staged"
    },
    "lint-staged": {
        "src/**/*.ts": "eslint --fix"
    },
    "dependencies": {
        "tree-sitter": "^0.20.0",
        "tree-sitter-c-sharp": "^0.19.0",
        "tree-sitter-go": "^0.19.1",
        "tree-sitter-java": "^0.19.1",
        "tree-sitter-javascript": "^0.19.0",
        "tree-sitter-kotlin": "^0.2.11",
        "tree-sitter-php": "^0.19.0",
        "tree-sitter-typescript": "^0.19.0",
        "yargs": "^17.2.1"
    },
    "devDependencies": {
        "@babel/core": "^7.16.0",
        "@babel/preset-env": "^7.16.0",
        "@babel/preset-typescript": "^7.16.0",
        "@types/jest": "^27.0.2",
        "@types/node": "^16.11.6",
        "@types/yargs": "^17.0.5",
        "@typescript-eslint/eslint-plugin": "^5.4.0",
        "@typescript-eslint/parser": "^5.4.0",
        "babel-jest": "^27.3.1",
        "eslint": "^8.2.0",
        "eslint-config-prettier": "^8.3.0",
        "husky": "^7.0.4",
        "jest": "^27.3.1",
        "lint-staged": "^12.0.2",
        "prettier": "^2.4.1",
        "pretty-quick": "^3.1.1",
        "typescript": "^4.4.4"
    }
}
