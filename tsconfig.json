        "sourceMap": true,
        "outDir": "dist",
        "resolveJsonModule": true,
        "strictFunctionTypes": true,
        "strictPropertyInitialization": true,
        "strictNullChecks": true,
        "types": ["vitest/globals"]
    },
    "exclude": ["resources", "node_modules"]
}
